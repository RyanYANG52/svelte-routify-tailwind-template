{
	"name": "svelte-app",
	"version": "1.0.0",
	"scripts": {
		"dev": "run-p routify rollup",
		"dev:nollup": "run-p routify nollup",
		"dev:sw": "cross-env SW=true npm run dev",
		"dev-dynamic": "cross-env BUNDLING=dynamic npm run dev",
		"build": "routify -b && rollup -c && spank",
		"build:sw": "cross-env SW=true npm run build",
		"serve": "spassr --serve-spa --serve-ssr",
		"export": "routify export",
		"build:docker": "npm run build && ./scripts/docker/docker-build.sh",
		"deploy:now": "cd scripts/now && npm run deploy",
		"deploy:netlify": "cd scripts/netlify && npm run deploy",
		"rollup": "rollup -cw",
		"nollup": "nollup -c",
		"routify": "routify",
		"validate": "svelte-check"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^15.1.0",
		"@rollup/plugin-node-resolve": "^9.0.0",
		"@rollup/plugin-replace": "^2.3.3",
		"autoprefixer": "^10.0.1",
		"concurrently": "^5.3.0",
		"cross-env": "^7.0.2",
		"cssnano": "^4.1.10",
		"del": "^6.0.0",
		"nollup": "^0.13.13",
		"npm-run-all": "^4.1.5",
		"rollup": "^2.32.1",
		"rollup-plugin-copy": "^3.3.0",
		"rollup-plugin-hot": "^0.1.1",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-postcss": "^3.1.8",
		"rollup-plugin-svelte": "^6.1.0",
		"rollup-plugin-svelte-hot": "^0.10.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-workbox": "^5.2.1",
		"spassr": "^2.1.5",
		"tailwindcss": "^1.9.5",
		"svelte": "^3.29.0",
		"svelte-check": "^1.0.60",
		"svelte-preprocess": "^4.5.1",
		"@rollup/plugin-typescript": "^6.0.0",
		"typescript": "^4.0.3",
		"tslib": "^2.0.3",
		"@tsconfig/svelte": "^1.0.10",
		"postcss": "^8.1.2",
		"postcss-import": "^13.0.0",
		"postcss-load-config": "^3.0.0",
		"stylelint": "^13.7.2",
		"stylelint-config-recommended": "^3.0.0",
		"eslint": "^7.11.0",
		"eslint-config-airbnb-typescript": "^12.0.0",
		"eslint-config-prettier": "^6.13.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-svelte3": "^2.7.3",
		"prettier": "^2.1.2"
	},
	"dependencies": {
		"@roxi/routify": "^2.1.0",
		"spank": "^1.4.0"
	},
	"routify": {
		"extensions": "svelte,html,svx,md",
		"dynamicImports": true,
		"routifyDir": ".routify"
	},
	"browserslist": [
		"defaults",
		"not IE 11",
		"not IE_Mob 11",
		"not edge 18"
	]
}
